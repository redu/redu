<% if log.logeable_type.eql?('User') || log.logeable_type.eql?('Education') || log.logeable_type.eql?('Experience') %>
  <%= render "statuses/logs/user", :log => log %>
<% elsif (log.logeable_type.eql? 'Friendship') %>
  <%= render "statuses/logs/friendship", :log => log %>
<% elsif log.logeable_type.eql? 'Course' %>
  <%= render "statuses/logs/course", :log => log %>
<% elsif log.logeable_type.eql? 'Space' %>
  <%= render "statuses/logs/space", :log => log %>
<% elsif log.logeable_type.eql? 'Subject' %>
  <%= render "statuses/logs/subject", :log => log %>
<% elsif log.logeable_type.eql? 'Lecture' %>
  <%= render "statuses/logs/lecture", :log => log %>
<% end %>
