<% if @memberships.empty? %>
  $content = $('<%= escape_javascript(render "shared/empty_message", icon_message: "Nenhum usuário encontrado.", icon_class: "icon-search-empty_128_130", message: "Nenhum usuário encontrado.") %>');
<% else %>
  $content = $('<%= escape_javascript(render "environments/admin/user_list_admin", memberships: @memberships, environment: @environment) %>');
<% end %>
$(".environment-management-member-search-result").html($content);